        "# count tokens\n",
        "model_count_tokens = \"bert-base-uncased\"\n",
        "tokenizer = AutoTokenizer.from_pretrained(model_count_tokens)\n",
        "\n",
        "def count_tokens(text):\n",
        "    # Tokenize the text\n",
        "    tokens = tokenizer.tokenize(text)\n",
        "    # Return the count of tokens\n",
        "    return len(tokens)\n",
